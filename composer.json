{
    "name": "bit3/git-php",
    "description": "Easy to use GIT wrapper for PHP.",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "psr/log": "^1.0",
        "symfony/process": "^4.0 || ^5.0"
    },
    "require-dev": {
        "symfony/filesystem": "^4.0 || ^5.0",
        "phpunit/phpunit": "7 - 9"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\GitPhp\\": "src",
            "Bit3\\GitPhp\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
